{
    "name": "snakeout",
    "version": "1.0.0",
    "main": "src/server/app.js",
    "scripts": {
        "start": "run-s build:prod start:prod",
        "dev": "run-p start:dev build:dev",
        "start:dev": "nodemon --exec npm run start:prod",
        "build:dev": "webpack --mode development --watch",
        "start:prod": "node src/server/bin/www",
        "build:prod": "webpack --mode production",
        "test": "tape src/server/test/*.js"
    },
    "author": "Tanner Krewson",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "^7.10.5",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "babel-loader": "^8.1.0",
        "bootstrap": "^4.5.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^4.1.0",
        "debug": "~4.1.1",
        "express": "^4.17.1",
        "html-webpack-plugin": "^4.3.0",
        "knuth-shuffle": "^1.0.8",
        "morgan": "~1.10.0",
        "npm-run-all": "^4.1.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-icons": "^3.10.0",
        "react-router-dom": "^5.2.0",
        "socket.io": "^2.4.0",
        "socket.io-client": "^2.3.0",
        "style-loader": "^1.2.1",
        "uuid": "^8.3.0",
        "webpack": "^4.44.0",
        "webpack-cli": "^3.3.12"
    },
    "devDependencies": {
        "exports-loader": "^1.1.0",
        "husky": "^4.2.5",
        "nodemon": "^2.0.4",
        "prettier": "2.0.5",
        "pretty-quick": "^2.0.1",
        "tape": "^5.0.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tannerkrewson/snakeout.git"
    },
    "bugs": {
        "url": "https://github.com/tannerkrewson/snakeout/issues"
    },
    "homepage": "https://snakeout.tannerkrewson.com/",
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
